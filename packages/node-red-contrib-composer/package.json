{
  "name": "node-red-contrib-composer",
  "version": "0.0.14",
  "description": "Hyperledger Composer nodes for node-red",
  "dependencies": {
    "composer-admin": "^0.20.7",
    "composer-client": "^0.20.7",
    "composer-common": "^0.20.7",
    "formidable": "1.1.1"
  },
  "devDependencies": {
    "eslint": "^3.5.0",
    "license-check-and-add": "2.3.1"
  },
  "engines": {
    "node": ">=8.9.5"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer-tools"
  },
  "keywords": [
    "node-red",
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "node-red": {
    "nodes": {
      "hperledger-composer": "nodes/hyperledger-composer.js",
      "hperledger-composer-config": "nodes/hyperledger-composer-config.js"
    }
  },
  "scripts": {
    "licchk": "license-check-and-add",
    "test": "npm run licchk"
  },
  "author": "Hyperledger Composer",
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "node_modules"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml"
    ],
    "insert_license": false,
    "license_formats": {
      "editorconfig": {
        "eachLine": {
          "prepend": "# "
        }
      },
      "js": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    }
  }
}
