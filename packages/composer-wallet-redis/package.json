{
  "name": "composer-wallet-redis",
  "version": "0.0.13",
  "description": "Hyperledger Composer Wallet implementation for Redis",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check-and-add",
    "test": "npm run redis:start && nyc mocha --recursive -t 10000",
    "mocha": "mocha --recursive -t 10000",
    "nyc": "nyc mocha --recursive -t 10000",
    "redis:start": "./startRedis.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer-wallet-redis.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-things": "0.2.0",
    "eslint": "^3.19.0",
    "license-check-and-add": "~2.3.0",
    "lodash.clonedeep": "4.5.0",
    "mocha": "3.4.2",
    "nyc": "^11.1.0",
    "sinon": "^2.3.8"
  },
  "dependencies": {
    "composer-common": "^0.17.3",
    "redis": "^2.8.0"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      ".nyc-output",
      "node_modules",
      "coverage"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml"
    ],
    "insert_license": false,
    "license_formats": {
      "js|css": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    },
    "output": "/Users/user/Desktop/wallet-out.txt"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**",
      "lib/shellcmds/**",
      "shell.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 97,
    "branches": 97,
    "functions": 97,
    "lines": 97
  }
}
