{
  "name": "@ampretia/composer-wallet-cloudant",
  "version": "0.2.0",
  "description": "Hyperledger Composer Wallet implementation for the IBM Cloud Object Store",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "test": "nyc mocha --recursive -t 10000",
    "mocha": "mocha --recursive -t 10000 test",
    "nyc": "nyc mocha --recursive -t 10000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ampretia/composer-wallet-cloudant.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-things": "0.2.0",
    "delay": "2.0.0",
    "eslint": "3.17.1",
    "license-check": "1.1.5",
    "lodash.clonedeep": "4.5.0",
    "mocha": "3.4.2",
    "nyc": "11.1.0",
    "sinon": "2.3.8"
  },
  "dependencies": {
    "@cloudant/cloudant": "2.1.0",
    "composer-common": "^0.19.8"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./cli.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**",
      "lib/shellcmds/**",
      "shell.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
